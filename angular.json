{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "libs",
  "projects": {
    "app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "apps/app",
      "sourceRoot": "apps/app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "apps/app/src/main.ts",
            "tsConfig": "apps/app/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/app/public"
              }
            ],
            "styles": ["apps/app/src/styles.css"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app:build:production"
            },
            "development": {
              "buildTarget": "app:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    },
    "feature-shell": {
      "projectType": "library",
      "root": "libs/app/shell/feature-shell",
      "sourceRoot": "libs/app/shell/feature-shell/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "libs/app/shell/feature-shell/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/app/shell/feature-shell/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "libs/app/shell/feature-shell/tsconfig.spec.json"
          }
        }
      }
    },
    "feature-welcome": {
      "projectType": "library",
      "root": "libs/app/home/feature-welcome",
      "sourceRoot": "libs/app/home/feature-welcome/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "libs/app/home/feature-welcome/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/app/home/feature-welcome/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "libs/app/home/feature-welcome/tsconfig.spec.json"
          }
        }
      }
    },
    "ui-layouts": {
      "projectType": "library",
      "root": "libs/app/layouts/ui-layouts",
      "sourceRoot": "libs/app/layouts/ui-layouts/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "libs/app/layouts/ui-layouts/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/app/layouts/ui-layouts/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "libs/app/layouts/ui-layouts/tsconfig.spec.json"
          }
        }
      }
    },
    "ui-navigation": {
      "projectType": "library",
      "root": "libs/app/layouts/ui-navigation",
      "sourceRoot": "libs/app/layouts/ui-navigation/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "libs/app/layouts/ui-navigation/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/app/layouts/ui-navigation/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "libs/app/layouts/ui-navigation/tsconfig.spec.json"
          }
        }
      }
    },
    "ui-hero": {
      "projectType": "library",
      "root": "libs/app/home/ui-hero",
      "sourceRoot": "libs/app/home/ui-hero/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "libs/app/home/ui-hero/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/app/home/ui-hero/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "libs/app/home/ui-hero/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": ["@tools/schematics"]
  }
}
