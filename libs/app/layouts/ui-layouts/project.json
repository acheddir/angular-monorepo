{
  "name": "ui-layouts",
  "projectType": "library",
  "prefix": "app",
  "architect": {
    "build": {
      "builder": "@angular/build:ng-packagr",
      "configurations": {
        "production": {
          "tsConfig": "libs/app/layouts/ui-layouts/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/app/layouts/ui-layouts/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "builder": "@angular/build:unit-test",
      "options": {
        "tsConfig": "libs/app/layouts/ui-layouts/tsconfig.spec.json"
      }
    }
  }
}
